{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    // Project Structure
    "baseUrl": ".",
    "outDir": "dist",
    "target": "es2020",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
