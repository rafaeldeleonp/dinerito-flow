{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "type-check": {
      "dependsOn": ["^type-check"],
      "outputs": []
    },
    "format": {
      "dependsOn": ["type-check", "^format"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["format", "^lint"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["lint"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "build": {
      "dependsOn": ["lint", "^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "clean": {
      "dependsOn": ["^clean"],
      "outputs": []
    }
  }
}
